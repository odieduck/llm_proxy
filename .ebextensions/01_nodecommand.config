option_settings:
  aws:elasticbeanstalk:container:nodejs:
    NodeCommand: "node src/index-dynamodb.js"
  aws:elasticbeanstalk:application:environment:
    NODE_ENV: production
    AWS_REGION: us-east-1
    DYNAMODB_USERS_TABLE: llm-proxy-users
    DYNAMODB_SESSIONS_TABLE: llm-proxy-sessions  
    DYNAMODB_USAGE_TABLE: llm-proxy-usage
    JWT_SECRET: production-jwt-secret-change-this